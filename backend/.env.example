# Example environment variables for development
# Copy to .env and customize for your environment

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# Docker
DOCKER_SOCKET_PATH=/var/run/docker.sock
DOCKER_POOL_SIZE=10
DOCKER_MAX_POOL_SIZE=20

# Security (set strong values in production)
# AES_KEY should be a base64 or hex encoded 32-byte key in production
AES_KEY=dev-aes-key-placeholder
SERVER_SECRET=dev-server-secret
SESSION_SECRET=dev-session-secret

# Paths to native tools (set to real paths in production)
DAP_ADAPTER_PATH=/usr/local/bin/OpenDebugAD7
GDB_PATH=/usr/bin/gdb

# App
NODE_ENV=development
